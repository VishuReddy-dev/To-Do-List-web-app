<%- include("header")-%>
    <div id="heading">
    <h1><%=Listtitle%></h1>
    </div>
    <div class="box">
        <%for(let i=0;i<items.length;i++){%>  
            <div class="item">
                <input type="checkbox">      
                <p><%=items[i]%></p>
            </div>   
        <%}  %>      
    <form action="/" method="post" class="item">
        <input type="text" name="newitem"placeholder="New item">
        <button type="submit" name="list" value="<%=Listtitle%>">+</button>
    </form>
    </div>
    <%- include("footer")-%>